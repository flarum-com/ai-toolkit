{
    "name": "blomstra/ai-toolkit",
    "description": "The chatgpt toolkit for your community.",
    "keywords": [
        "extension",
        "flarum",
        "blomstra",
        "ai",
        "chatgpt",
        "openai"
    ],
    "license": "mit",
    "type": "flarum-extension",
    "authors": [
        {
            "name": "DaniÃ«l Klabbers",
            "email": "daniel@klabbers.email",
            "homepage": "https://luceos.com"
        }
    ],
    "require": {
        "php": ">= 8.2",
        "flarum/core": "^1.8.0",
        "openai-php/client": "^v0.7.10"
    },
    "autoload": {
        "psr-4": {
            "Blomstra\\Ai\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^6.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Blomstra\\Ai\\Tests\\": "tests/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "AI toolkit",
            "category": "feature",
            "icon": {
                "image": "resources/assets/logo.png",
                "backgroundColor": "#E3E7F1",
                "backgroundSize": "60%",
                "backgroundRepeat": "no-repeat",
                "backgroundPosition": "center"
            }
        },
        "extiverse": {
            "discuss": ""
        }
    }
}
